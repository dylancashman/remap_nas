{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": false
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/no-use-v-if-with-v-for": "off"
    }
}